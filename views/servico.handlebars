<link rel="stylesheet" href="../css/servico.css">
{{#each servicos}}
<form action="/contratar" method="POST">
<input type="hidden" value="{{dataValues.id}}">
<br> <br>
<div id="lista">
<nav class="listagem">
<li>{{dataValues.nome}}</li>
Profissional: {{dataValues.profissional}} <br>
Valor: ${{dataValues.preco}},00
</nav>
<br>
<button class="btn btn-primary contratar">Contratar</button>
</div>
<br>
</form>
{{/each}}
